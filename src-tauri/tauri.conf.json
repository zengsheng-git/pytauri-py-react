{
  "$schema": "../node_modules/@tauri-apps/cli/config.schema.json",
  "productName": "pytauri-react-starter",
  "version": "0.1.0",
  "identifier": "com.pytauri-react-starter.app",
  "build": {
    "frontendDist": "../build",
    "devUrl": "http://localhost:1420",
    "beforeDevCommand": "npm run dev",
    "beforeBuildCommand": "npm run build"
  },
  "app": {
    "windows": [
      {
        "title": "pytauri-react-starter",
        "width": 800,
        "height": 600,
        "resizable": true,
        "fullscreen": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all",
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.icns",
      "icons/icon.ico"
    ],
    "resources": [
      "../pysrc",
      "../main.py",
      "../Tauri.toml",
      "../capabilities",
      "../build"
    ]
  },
  "plugins": {
    "pytauri": {
      "enabled": true
    }
  }
}
